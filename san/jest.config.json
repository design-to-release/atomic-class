{
  "coverageDirectory": "coverage",
  "coverageProvider": "v8",
  "preset": "ts-jest",
  "testMatch": [
    "<rootDir>/src/**/*.spec.{ts,tsx,js,jsx}",
    "<rootDir>/src/*.spec.{ts,tsx,js,jsx}"
  ],
  "moduleFileExtensions": ["ts", "tsx", "js", "jsx", "json", "node"],
  "testEnvironment": "jsdom"
}
