{
  "name": "@atomic-class/core",
  "version": "0.1.1",
  "description": "core of atomic-class",
  "main": "dist/umd.js",
  "browser": "dist/umd.js",
  "esnext": "dist/es.js",
  "scripts": {
    "build": "npm run build:umd && npm run build:es",
    "build:umd": "rollup -c ../rollup.config.js --environment format:umd",
    "build:es": "rollup -c ../rollup.config.js --environment format:es",
    "dev": "rollup -c ../rollup.config.js -w",
    "start": "sirv public --no-clear"
  },
  "dependencies": {
    "@rollup/pluginutils": "^3.0.10",
    "acorn": "^8.4.1",
    "astring": "^1.7.5",
    "magic-string": "^0.25.7",
    "tslib": "^2.3.0"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^17.0.0",
    "@rollup/plugin-node-resolve": "^11.0.0",
    "@rollup/plugin-typescript": "^8.2.3",
    "only-allow": "^1.0.0",
    "rollup": "^2.3.4",
    "rollup-plugin-css-only": "^3.1.0",
    "rollup-plugin-livereload": "^2.0.0",
    "rollup-plugin-terser": "^7.0.0",
    "sirv-cli": "^1.0.0",
    "typescript": "^4.3.5"
  },
  "keywords": [
    "atomic-class"
  ],
  "homepage": "https://github.com/design-to-release/atomic-class",
  "repository": "design-to-release/atomic-class",
  "license": "MIT",
  "files": [
    "dist",
    "*.json",
    "*.md"
  ]
}
